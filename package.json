  "peerDependencies": {
    "formik": ">=2",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "react-fast-compare": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
