  "repository": {
    "type": "git",
    "url": "https://github.com/TylerShin/formik-persist-state.git"
  },
  "bugs": {
    "url": "https://github.com/TylerShin/formik-persist-state/issues"
  },
  "homepage": "https://github.com/TylerShin/formik-persist-state",
  "keywords": [
    "formik",
